<script xml:space="preserve">
/*<![CDATA[*/
 $.views.helpers({
	 arriveDay : function(arrive_day_diff,running_time) {
	 	if('--'==running_time){
			return "";
		}else if('0'==arrive_day_diff){
			return "当日到达";
		}else if('1'==arrive_day_diff){
            return "次日到达";
		}else if('2'==arrive_day_diff){
			return "第三日到达";
		}else{
			return "第四日到达";
		}
	},
	changeLiShi : function(value) {
		if (value.substring(0,1) == "0") {
			if (value.substring(1,2) == "0") {
			    if (value.substring(3,4) == "0") {
				    value = value.substring(4,5) + "分";
			    } else {
			    	value = value.substring(3,5) + "分";
				}
			} else {
				value = value.substring(1,2) + "小时" + value.substring(3,5) + "分";
			}
		} else {
			if (value.substring(3,5) == "00") {
				value = value.substring(0,2) + "小时";
			} else {
			    value = value.substring(0,2) + "小时" + value.substring(3,5) + "分";;
			}
		}
		return value;
	},
	isNum : function(value) {
		if(''==value||'--'==value||'无'==value){
			return 0;
		}
		//return parseInt(value);
		return 1;
	},
	returnStr : function() {
		
		return '------';
	}
 });
 /*]]>*/
</script>
