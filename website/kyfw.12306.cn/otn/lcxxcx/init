<script xml:space="preserve">
/*<![CDATA[*/
 $.views.helpers({
	 arriveDay : function(arrive_day_diff) {
		if('0'==arrive_day_diff){
			return "当日到达";
		}else if('1'==arrive_day_diff){
            return "次日到达";
		}else if('2'==arrive_day_diff){
			return "第三日到达";
		}else{
			return "第四日到达";
		}
	},
	changeLiShi : function(value) {
		if (value.substring(0,1) == "0") {
			if (value.substring(1,2) == "0") {
			    if (value.substring(3,4) == "0") {
				    value = value.substring(4,5) + "分";
			    } else {
			    	value = value.substring(3,5) + "分";
				}
			} else {
				value = value.substring(1,2) + "小时" + value.substring(3,5) + "分";
			}
		} else {
			if (value.substring(3,5) == "00") {
				value = value.substring(0,2) + "小时";
			} else {
			    value = value.substring(0,2) + "小时" + value.substring(3,5) + "分";;
			}
		}
		return value;
	},
	isNum : function(value) {
		if(''==value||'--'==value||'无'==value){
			return 0;
		}
		//return parseInt(value);
		return 1;
	},
	isZK : function(key,value) {
		var _index=value?value.indexOf(key):-1;
		var flag=false;
		if(_index>-1&&(_index%2)==0){
			flag=true;
		}
		return flag;
	},
	returnStr : function() {
		
		return '------';
	}
 });
 var dateArr =['02-24','02-25','02-26','02-27','02-28','03-01','03-02','03-03','03-04','03-05','03-06','03-07','03-08','03-09','03-10','03-11','03-12','03-13','03-14','03-15','03-16','03-17','03-18','03-19','03-20','03-21','03-22','03-23','03-24','03-25'];
 var fullDateArr =['2017-02-24','2017-02-25','2017-02-26','2017-02-27','2017-02-28','2017-03-01','2017-03-02','2017-03-03','2017-03-04','2017-03-05','2017-03-06','2017-03-07','2017-03-08','2017-03-09','2017-03-10','2017-03-11','2017-03-12','2017-03-13','2017-03-14','2017-03-15','2017-03-16','2017-03-17','2017-03-18','2017-03-19','2017-03-20','2017-03-21','2017-03-22','2017-03-23','2017-03-24','2017-03-25'];
 var initFromDate = '2017-02-24';
 /*]]>*/
</script>
